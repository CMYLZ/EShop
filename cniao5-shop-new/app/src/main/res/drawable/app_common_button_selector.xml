<?xml version="1.0" encoding="utf-8"?>

<!-- 
	登录按钮背景
	
 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
        	<corners
		        android:radius="3dip"/>
		    <padding
		        android:top="7dip"
		        android:bottom="7dip"/>
		    <solid
		        android:color="#09aa4c"/>
		</shape>
    </item>
    <item >
        <shape>
		    <corners
		        android:radius="3dip"/>
		    <padding
		        android:top="7dip"
		        android:bottom="7dip"/>
		    <solid
		        android:color="@color/app_main_color"/>
		</shape>
    </item>
</selector>
